<!DOCTYPE html>
<html>
<link rel="manifest" href="/manifest.json">
<%= favicon_link_tag 'logo.png' %>
  <head>
    <title>PraApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "lists/navbar" %>
    <div class="container">
      <%= render "lists/flash"%>
    <%= yield %>
    </div>
  </body>
</html>
